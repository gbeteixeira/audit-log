{
	"name": "audit-log",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/infra/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cov": "vitest run --coverage",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
		"test:e2e": "vitest run --config ./vitest.config.e2e.ts",
		"test:e2e:watch": "vitest --config ./vitest.config.e2e.ts",
		"test:ui": "vitest --ui",
		"prepare": "husky || true",
		"release": "release-it",
		"run:setup": "docker compose up -d && pnpm install && npx prisma migrate dev && npx prisma generate",
		"reset:hard": "pnpm run clean && npx prisma migrate reset --force"
	},
	"dependencies": {
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.2.2",
		"@nestjs/core": "^10.0.0",
		"@nestjs/platform-express": "^10.0.0",
		"@prisma/client": "^5.14.0",
		"multer": "1.4.5-lts.1",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"zod": "^3.23.8",
		"zod-validation-error": "^3.3.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@commitlint/types": "^19.0.3",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@release-it/conventional-changelog": "8.0.1",
		"@swc/core": "^1.5.7",
		"@types/express": "^4.17.17",
		"@types/multer": "^1.4.11",
		"@types/node": "^20.12.12",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitest/coverage-v8": "^1.6.0",
		"commitlint": "^19.3.0",
		"dotenv": "^16.4.5",
		"eslint": "^8.42.0",
		"husky": "^9.0.11",
		"inquirer": "9",
		"lint-staged": "^15.2.4",
		"prisma": "^5.14.0",
		"release-it": "17.3.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3",
		"unplugin-swc": "^1.4.5",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0",
		"eslint-config-prettier": "^9.1.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.2.1",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.14"
	},
	"release-it": {
		"git": {
			"commitMessage": "chore: release v${version}"
		},
		"github": {
			"release": true
		},
		"npm": {
			"publish": false
		},
		"plugins": {
			"@release-it/conventional-changelog": {
				"infile": "CHANGELOG.md",
				"preset": {
					"name": "conventionalcommits",
					"types": [
						{
							"type": "chore(deps)",
							"section": "Dependency Upgrades"
						},
						{
							"type": "fix(deps)",
							"section": "Dependency Upgrades"
						},
						{
							"type": "feat",
							"section": "Features"
						},
						{
							"type": "fix",
							"section": "Bug Fixes"
						},
						{
							"type": "perf",
							"section": "Performance Improvements"
						},
						{
							"type": "revert",
							"section": "Reverts"
						},
						{
							"type": "docs",
							"section": "Documentation"
						},
						{
							"type": "refactor",
							"section": "Code Refactoring"
						},
						{
							"type": "test",
							"section": "Tests"
						},
						{
							"type": "ci",
							"section": "Continuous Integration"
						}
					]
				}
			}
		}
	}
}
